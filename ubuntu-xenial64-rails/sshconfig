Host web
  HostName 192.168.33.10
  User vagrant
  Port 22
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile .vagrant/machines/db/virtualbox/private_key
  IdentitiesOnly yes
  LogLevel FATAL

